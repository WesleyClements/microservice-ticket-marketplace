{
  "name": "tickets",
  "version": "1.0.0",
  "description": "Ticket service",
  "author": "Wesley Clements <wesley.clements@gmail.com>",
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "start": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run start:prod; else npm run start:dev; fi",
    "start:prod": "ts-node src/index.ts",
    "start:dev": "ts-node-dev --poll -r tsconfig-paths/register src/index.ts",
    "test": "jest --watchAll --no-cache"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^app$": "<rootDir>/src/app",
      "^db(.*)$": "<rootDir>/src/db$1",
      "^errors(.*)$": "<rootDir>/src/errors$1",
      "^middleware(.*)$": "<rootDir>/src/middleware$1",
      "^routes(.*)$": "<rootDir>/src/routes$1",
      "^util/(.*)$": "<rootDir>/src/util/$1"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/supertest": "^2.0.10",
    "jest": "^26.5.2",
    "mongodb-memory-server": "^6.9.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.1"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-session": "^2.0.41",
    "@types/express": "^4.17.8",
    "@types/express-validator": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.11.8",
    "@wkctickets/common": "^0.0.7",
    "bcrypt": "^5.0.0",
    "cookie-session": "^1.4.0",
    "express": "^5.0.0-alpha.8",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  }
}